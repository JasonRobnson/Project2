{{> navbar}}

<div class="container">
    <div class="row">
        <div class="col s12">
            <h2>My Profile</h2>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col s12 m8 20 offset-m2 offset-l0">
            <div class="row">
                {{users}}
                <div class="card-panel grey lighten-5 overflow-auto">
                    <div class="row">
                        <div class="col s6">
                            <ul>
                                <li>
                                    <img src="/images/profile.jpg" class="circle">
                                </li>
                            </ul>
                        </div>
                        <div class="col s6">
                            <ul>
                                <li>Name:
                                    <span id="firstname">Bilbo</span>
                                    <span id="lastName"> Wilson</span>
                                </li>
                                <li>User Name:
                                    <span id="userName">John123</span>
                                </li>
                                <li>Unit Number:
                                    <span id="userName">123</span>
                                </li>
                                <li>Email:
                                    <span id="email">johnwilson@gmail.com</span>
                                </li>
                                <li>Lease End:
                                    <span id="leaseEnd">12/31/2018</span>
                                </li>
                                <li>Phone:
                                    <span id="phone">(612) 111-2222</span>
                                </li>
                                <li>Interest:
                                    <span id="interest">Music</span>
                                </li>
                            </ul>
                        </div>
                        <div class="center-align">
                            <button class="btn orange darken-3 waves-effect waves-orange" type="submit" id="profile-edit-button">
                                <i class="material-icons right">edit</i>Edit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>